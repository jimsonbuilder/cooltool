/**index.wxss**/
.container {
  background: url(data:image/jpg;base64,) no-repeat;
  background-color: skyblue;
  background-size: cover;
}

/* 用户信息 */
.userinfo {
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.userinfo-avatar {
  width: 100rpx;
  height: 100rpx;
  margin: 5rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #aaa;
  font-size: 13px;
}

/* 天气信息 */
.temperature-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  color: white;
}
.temperature-city {
  font-size: 30px;
  font-weight: bold;
}
.temperature-number {
  font-size: 40px;
  position: relative;
  top: -100rpx;
}
.temperature-text {
  font-size: 20px;
  font-weight: bold;
  position: relative;
  top: -80rpx;
}
.temperature-img {
  width: 150rpx;
}

/* 选择城市 */
.choose-city-picker {
  position: absolute;
  left: 20rpx;
  top: 25rpx;
}
.choose-city-picker .choose-city-btn {
  width: 150rpx;
  font-size: 14px;
  padding: 5rpx 10rpx;
}

/* 输入框查询天气 */
.search-temperature {
  background: #eee;
  height: 70rpx;
  color: #666;
  border-radius: 15rpx;
  padding: 0 20rpx;
  position: fixed;
  top: 22rpx;
  margin-left: 10rpx;
  font-size: 13px;
}