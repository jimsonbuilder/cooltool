/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0 360rpx;
  box-sizing: border-box;
}

/* 引入阿里巴巴iconfont */
@font-face {
  font-family: 'iconfont';
  src: url('//at.alicdn.com/t/font_1749242_cgww757vcf.eot');
  src: url('//at.alicdn.com/t/font_1749242_cgww757vcf.eot?#iefix') format('embedded-opentype'),
    url('//at.alicdn.com/t/font_1749242_cgww757vcf.woff2') format('woff2'),
    url('//at.alicdn.com/t/font_1749242_cgww757vcf.woff') format('woff'),
    url('//at.alicdn.com/t/font_1749242_cgww757vcf.ttf') format('truetype'),
    url('//at.alicdn.com/t/font_1749242_cgww757vcf.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 60rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/*本项目使用的2个图标 */
.icon-zanting:before {
  content: '\e667';
}
.icon-bofang:before {
  content: '\e60c';
}